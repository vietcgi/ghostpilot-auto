{
  "name": "ghostpilot-auto",
  "version": "0.1.0",
  "description": "Antidetect browser with automation using Electron and Puppeteer",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "dependencies": {
    "electron": "^27.0.0",
    "puppeteer-core": "^21.3.8",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "express": "^4.18.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "fs-extra": "^11.1.1",
    "puppeteer": "^21.3.8"
  },
  "devDependencies": {},
  "build": {
    "appId": "com.ghostpilot.auto",
    "productName": "GhostPilot Auto",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*"
    ],
    "mac": {
      "target": "dmg",
      "category": "public.app-category.utilities"
    }
  },
  "author": "Kevin Vu",
  "license": "MIT"
}